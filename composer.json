{
  "name": "mkedo/php-travis-coveralls",
  "description": "PHP, travis ci, coveralls example",
  "license": "BSD-3-Clause",
  "require": {
    "php": ">=5.5"
  },
  "autoload": {
    "psr-4": {
      "MyApp\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.27",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "scripts": {
    "test": "phpunit",
    "cs": "phpcs --standard=PSR2 --encoding=utf-8 src/ tests/"
  }
}
